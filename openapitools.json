{
  "$schema": "./node_modules/@openapitools/openapi-generator-cli/config.schema.json",
  "spaces": 2,
  "generator-cli": {
    "version": "7.18.0",
    "useDocker": true,
    "generators": {
      "typescript-axios": {
        "inputSpec": "openapi/openapi.json",
        "output": "src/shared/api",
        "generatorName": "typescript-axios",
        "additionalProperties": {
          "supportsES6": "true"
        }
      }
    }
  }
}
